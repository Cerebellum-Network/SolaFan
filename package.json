  "private": true,
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "export PORT=3004&&node scripts/start.js",
    "build": "node --max-old-space-size=3072 scripts/build.js",
    "test": "node scripts/test.js --watchAll=false",
    "update-snapshot": "node scripts/test.js --watchAll=false --updateSnapshot",
    "test:e2e": "npx playwright test",
    "lint:js": "eslint --ext=.js,.ts,.tsx .",
    "lint:js:fix": "eslint --ext=.js,.ts,.tsx . --fix",
    "lint:type-check": "tsc --noEmit",
    "format": "prettier '**/*' --write --ignore-unknown --loglevel warn",
    "codestyle": "prettier '**/*' --check --ignore-unknown",
    "lint": "concurrently \"npm run lint:js\" \"npm run lint:type-check\"",
    "refactor": "prettier --write '**/*.{js,json,jsx,ts,tsx,md,css}' --ignore-path .eslintignore",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "extract-copies": "i18next 'src/**/*.{js,jsx,tsx,ts}' --config ./i18next-parser.config.js",
    "extract-copies-check": "i18next 'src/**/*.{js,jsx,tsx,ts}' --config ./i18next-parser.config.js --fail-on-update"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run refactor"
    }
  },
